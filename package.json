{
  "name": "fp-redux-websocket",
  "version": "1.0.0",
  "description": "Middleware for use websocket with Redux. You can dispatch action directly from server.",
  "main": "lib/index.js",
  "scripts": {
    "flow": "flow",
    "lint": "eslint src tests",
    "pretest": "npm run lint & npm run flow",
    "test": "mocha --compilers js:babel-core/register tests/*.js",
    "babel": "babel src/ -d lib",
    "prepack": "npm run test & npm run babel",
    "report-coverage": "./node_modules/.bin/nyc npm test && ./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "homepage": "https://pasalino.com/redux-persistent-websocket/",
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pasalino/fp-redux-websocket"
  },
  "keywords": [
    "redux",
    "websocket",
    "middleware",
    "redux-middleware"
  ],
  "author": "Pasqualino de Simone <pasalino@gmail.com>",
  "constributors": [
    {
      "name": "Federico Blancato",
      "email": "federico.blancato@thesnake.it"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-remove-comments": "^2.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.77.0",
    "flowtype": "^2.0.0",
    "husky": "^1.0.0-rc.13",
    "mocha": "latest",
    "nyc": "^12.0.2",
    "redux": "^4.0.0"
  }
}
